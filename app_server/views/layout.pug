doctype html
html(data-bs-theme = "dark" lang='en')
  head
    title= title
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous")
  body
    nav.navbar.fixed-top.navbar-expand-lg.bg-body-tertiary 
      .container-fluid 
        a.navbar-brand(href="/") archRouter
        button.navbar-toggler(type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation")
          span.navbar-toggler-icon 
        .collapse.navbar-collapse(id="navbarNav")
          ul.navbar-nav.justify-content-start
            li.nav-item 
              a.nav-link(aria-current="page" href="/about") About
        <form class="d-flex" role="search" method = "post">
          <input class="form-control me-2" type="text" id = "lat" placeholder="Latitude" aria-label="Latitude">
          <input class="form-control me-2" type="text" id = "long" placeholder="Longitude" aria-label="Longitude">
          <button class="btn btn-outline-success" type="submit" onclick="saveFile()">Search</button>
        </form>
    .container-fluid
      block content
    script(src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.7/dist/umd/popper.min.js" integrity="sha384-zYPOMqeu1DAVkHiLqWBUTcbYfZ8osu1Nd6Z89ify25QV9guujx43ITvfi12/QExE" crossorigin="anonymous")
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.min.js" integrity="sha384-Y4oOpwW3duJdCWv5ly8SCFYWqFDsfob/3GkgExXKV4idmbt98QcxXYs9UoXAB7BZ" crossorigin="anonymous")  
    script(src="/formTotext.js") 